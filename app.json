{
  "expo": {
    "name": "Desconto Sustent√°vel",
    "slug": "desconto-sustentavel",
    "scheme": "app",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#F54142",
    "splash": {
      "image": "./assets/splash-icon.png",
      "backgroundColor": "#F54142"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.citec.descontosustentavel"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F54142"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.citec.descontosustentavel"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-updates",
        {}
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "30dfc7d9-fda5-4b82-8755-114b2dece766"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {}
  }
}
